] = useState(false);\n    const [text, setTextState] = useState(routine.routine);\n\n    const input = useRef();\n    const iconBtn = useRef();\n    const btns = useRef();\n\n    const showBtns = () => {\n      setMenuState(!menuActive);\n      setFormState(false);\n    };\n\n    const showform = () => {\n      setFormState(!formActive);\n    };\n\n    const onChange = e => {\n      setTextState(e.target.value);\n    };\n    const onSubmit = e => {\n      e.preventDefault();\n    };\n\n    return (\n      <li className=\"Routine-list\" key={routine.id}>\n        <form className={formActive ? 'show' : ''} onSubmit={onSubmit}>\n          <input type=\"text\" value={text} name=\"modifyText\" ref={input} onChange={onChange} />\n          <button\n            onClick={() => {\n              editRoutine(routine.id, input.current.value);\n            }}\n          >\n            수정완료\n          </button>\n        </form>\n        <p className={!formActive ? 'show' : ''}>{routine.routine}</p>\n        <time style={{ fontWeight: 'bold' }}>\n          {routine.startTime} ~ {routine.endTime} <StyleDay days={routine.day} />\n        </time>\n        <div className={`Routine-btn ${menuActive ? 'show' : ''}`} ref={btns}>\n          <button onClick={showform}>수정</button>\n          <button\n            onClick={() => {\n              removeRoutine(routine.id);\n            }}\n          >\n            삭제\n          </button>\n        </div>\n        <button ref={iconBtn} onClick={showBtns}>\n          <FiMoreHorizontal />\n        </button>\n      </li>\n    );\n  };\n\n  return routines.map(routine => <RoutineLi routine={routine} />);\n};\nlet timeId = time => {\n  setTimeout(() => {\n    console.log('알람입니다');\n  }, time);\n};\n\nconst today = new Date();\nlet hour = today.getHours();\nconst nowTime =\n  hour > 6 && hour < 12\n    ? '아침'\n    : hour > 12 && hour < 18\n    ? '오후'\n    : hour > 18 && hour < 21\n    ? '저녁'\n    : '밤';\n\nconst messages = [\n  `평온한 ${nowTime} 입니다  :)`,\n  `어떤 ${nowTime}시간을 보내고 계신가요?`,\n  '나른함을 깨워줄 음악을 들어보세요!',\n  '루티너와 함께 일상을 변화시켜요!',\n  '좋은 변화는 좋은 습관에서 시작해요!',\n  '작은 좋은 습관이 큰 변화를 만듭니다.',\n];\n\nconst randomItem = messages[Math.floor(Math.random() * messages.length)];\n\nconst Routine = ({ routines, getRoutine, onLogout, history, removeRoutine, editRoutine }) => {\n  useEffect(() => {\n    getRoutine();\n  }, []);\n  console.log(history);\n\n  const logout = () => {\n    onLogout();\n    localStorage.removeItem('token');\n  };\n  const click = () => {\n    history.push('/newroutine');\n  };\n  return (\n    <div className=\"Routine\">\n      <div className=\"header\">\n        <time>\n          <Moment interval={1000} format=\"M.DD (dd) hh:mm A\" />\n        </time>\n        <h1>\n          <p>{randomItem}</p>\n        </h1>\n        <div className=\"logoutbtn\">\n          <button onClick={logout}>로그아웃</button>\n        </div>\n      </div>\n      <ul className=\"section\">\n        <Lilist routines={routines} removeRoutine={removeRoutine} editRoutine={editRoutine} />\n      </ul>\n      <div className=\"plus\">\n        <button onClick={click}>\n          <BsPlus />\n        </button>\n      </div>\n      <nav className=\"navigation\">\n        <ul>\n          <li>\n            <button>\n              <MdAccessTime />\n            </button>\n          </li>\n          <li>\n            <button>\n              <GiHistogram />\n            </button>\n          </li>\n          <li>\n            <button>\n              <BsFillPersonFill />\n            </button>\n          </li>\n        </ul>\n      </nav>\n    </div>\n  );\n};\n\nexport default Routine;\n","/Users/apple/Desktop/progreser/src/modules/services/AuthService.js",[],"/Users/apple/Desktop/progreser/src/ModalPortal/Modal.js",[],"/Users/apple/Desktop/progreser-project/src/index.js",["199","200","201","202"],"/Users/apple/Desktop/progreser-project/src/App.js",["203"],"/Users/apple/Desktop/progreser-project/src/reportWebVitals.js",[],"/Users/apple/Deskto